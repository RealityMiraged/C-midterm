#include <iostream>
#include <vector>
using namespace std;

int main() {
  	vector<int> yippie;
  	yippie.push_back(1);
  	yippie.push_back(2);
  	yippie.push_back(3);
  	
	for(int i=0;i < yippie.size(); i++){
		cout<<yippie[i]<<" ";
	}
	cout<<endl;
	
	yippie.pop_back();
	
	cout<<"the reuslt will be:";
	for(int i=0;i < yippie.size(); i++){
		cout<<yippie[i]<<" ";
	}		
	return 0;
}
